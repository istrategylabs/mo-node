{
  "name": "{{cookiecutter.package_name}}",
  "version": "{{cookiecutter.version}}",
  "description": "{{cookiecutter.project_name}} - {{cookiecutter.description}}",
  "main": "index.js",
  "scripts": {
    "test": "node test/test.js",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/istrategylabs/{{cookiecutter.repo_name}}.git"
  },
  "dependencies": {
    "async": "^1.4.2",
    "bluebird": "^2.9.34"
    {% if cookiecutter.use_redis == 'y' -%}
    ,"redis": "^2.3.0"
    {%- endif %}
    {% if cookiecutter.use_express == 'y' -%}
    ,"express": "^4.13.3",
    "express-session": "^1.11.3"
    {%- endif %}
    {% if cookiecutter.use_postgres == 'y' -%}
    ,"bookshelf": "^0.8.1"
    {%- endif %}
  },
  "devDependencies": {
    "tape": "^4.2.0"
  },
  "engineStrict": true,
  "engines": {
    "npm": "^{{cookiecutter.npm_version}}",
    "node": "^{{cookiecutter.node_version}}"
  },
  "author": "{{cookiecutter.authro_name}} <{{cookiecutter.email}}>",
  "license": "MIT"
}
